def returnSorted(*args):
    return tuple(sorted(args))


x = returnSorted(1, 2, 34, 5, 7, 5, 4, 3, 4, 6, 7, 8, 1, 7, 6, 5, 4, 43, 4, 4, 5, 6, 99)
print(x)


def checkDict(dict1,**kwargs):
    if kwargs == dict1:
        return True
    else: return False


sampleDict = {'key_1':'Value', 'Key_2':2, 'keyZero':0}
print(checkDict(sampleDict,keyZero=0, key_1='Value', Key_2=2))
